
{
  "dashboard": {
    "id": null,
    "title": "AI Routing System Dashboard",
    "description": "Comprehensive monitoring dashboard for the AI Employee Platform routing system",
    "tags": ["ai-routing", "monitoring", "performance"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "ai_routing_requests_total",
            "legendFormat": "Total Requests"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"displayMode": "basic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "rate(ai_routing_requests_success_total[5m]) / rate(ai_routing_requests_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 3,
        "title": "Average Latency",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "ai_routing_request_duration_seconds{quantile=\"0.5\"} * 1000",
            "legendFormat": "Median Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 2000},
                {"color": "red", "value": 5000}
              ]
            },
            "unit": "ms"
          }
        }
      },
      {
        "id": 4,
        "title": "Active Providers",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "count(ai_routing_provider_health{status=\"healthy\"})",
            "legendFormat": "Healthy Providers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(ai_routing_requests_total[1m])",
            "legendFormat": "Requests/sec"
          },
          {
            "expr": "rate(ai_routing_requests_success_total[1m])",
            "legendFormat": "Successful Requests/sec"
          },
          {
            "expr": "rate(ai_routing_requests_error_total[1m])",
            "legendFormat": "Failed Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "legend": {
          "show": true,
          "values": false,
          "alignAsTable": false
        }
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, ai_routing_request_duration_seconds_bucket) * 1000",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.90, ai_routing_request_duration_seconds_bucket) * 1000",
            "legendFormat": "90th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, ai_routing_request_duration_seconds_bucket) * 1000",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, ai_routing_request_duration_seconds_bucket) * 1000",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (ms)",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "Provider Health Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "ai_routing_provider_health",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"__name__": true, "job": true, "instance": true},
              "renameByValue": {
                "provider": "Provider",
                "status": "Status",
                "health_score": "Health Score",
                "avg_latency": "Avg Latency (ms)"
              }
            }
          }
        ]
      },
      {
        "id": 8,
        "title": "Cost Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "rate(ai_routing_cost_total[5m])",
            "legendFormat": "Cost/min ($)"
          },
          {
            "expr": "ai_routing_budget_remaining",
            "legendFormat": "Budget Remaining ($)"
          }
        ],
        "yAxes": [
          {
            "label": "Cost ($)",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "Error Rate by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(ai_routing_errors_total{error_type=\"timeout\"}[5m])",
            "legendFormat": "Timeout Errors"
          },
          {
            "expr": "rate(ai_routing_errors_total{error_type=\"rate_limit\"}[5m])",
            "legendFormat": "Rate Limit Errors"
          },
          {
            "expr": "rate(ai_routing_errors_total{error_type=\"auth\"}[5m])",
            "legendFormat": "Auth Errors"
          },
          {
            "expr": "rate(ai_routing_errors_total{error_type=\"provider\"}[5m])",
            "legendFormat": "Provider Errors"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 10,
        "title": "Quality Scores",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "ai_routing_quality_score_avg",
            "legendFormat": "Average Quality Score"
          },
          {
            "expr": "ai_routing_quality_score{quantile=\"0.5\"}",
            "legendFormat": "Median Quality Score"
          },
          {
            "expr": "ai_routing_quality_threshold",
            "legendFormat": "Quality Threshold"
          }
        ],
        "yAxes": [
          {
            "label": "Quality Score",
            "min": 0,
            "max": 10
          }
        ]
      },
      {
        "id": 11,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(ai_routing_cache_hits_total[5m]) / (rate(ai_routing_cache_hits_total[5m]) + rate(ai_routing_cache_misses_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate (%)"
          },
          {
            "expr": "ai_routing_cache_size",
            "legendFormat": "Cache Size"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage / Count",
            "min": 0
          }
        ]
      },
      {
        "id": 12,
        "title": "System Resources",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"ai-routing-service\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          },
          {
            "expr": "rate(process_cpu_seconds_total{job=\"ai-routing-service\"}[5m]) * 100",
            "legendFormat": "CPU Usage (%)"
          },
          {
            "expr": "ai_routing_active_connections",
            "legendFormat": "Active Connections"
          }
        ],
        "yAxes": [
          {
            "label": "Usage",
            "min": 0
          }
        ]
      },
      {
        "id": 13,
        "title": "Request Distribution by Provider",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "sum by (provider) (rate(ai_routing_requests_total[5m]))",
            "legendFormat": "{{provider}}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "pieType": "pie",
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 14,
        "title": "Model Usage Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 36},
        "targets": [
          {
            "expr": "sum by (model) (rate(ai_routing_requests_total[5m]))",
            "legendFormat": "{{model}}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "pieType": "donut",
          "displayLabels": ["name", "value"]
        }
      },
      {
        "id": 15,
        "title": "Top Users by Request Volume",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 36},
        "targets": [
          {
            "expr": "topk(10, sum by (user_id) (rate(ai_routing_requests_total[1h])))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"__name__": true, "job": true, "instance": true},
              "renameByValue": {
                "user_id": "User ID",
                "Value": "Requests/hour"
              }
            }
          }
        ]
      },
      {
        "id": 16,
        "title": "Alert Status",
        "type": "table",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 44},
        "targets": [
          {
            "expr": "ALERTS{job=\"ai-routing-service\"}",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"__name__": true, "job": true, "instance": true},
              "renameByValue": {
                "alertname": "Alert Name",
                "alertstate": "State",
                "severity": "Severity",
                "description": "Description"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "-- Grafana --",
          "type": "dashboard",
          "iconColor": "rgba(0, 211, 255, 1)",
          "enable": true
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "provider",
          "type": "query",
          "query": "label_values(ai_routing_requests_total, provider)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "model",
          "type": "query", 
          "query": "label_values(ai_routing_requests_total, model)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    }
  },
  "overwrite": false
}
